public class LargestElement {

    public static void main(String[] args) {

        int[] arr = {3, 7, 2, 9, 5};

        int largest = arr[0];  // assume first element is largest

        for (int i = 1; i < arr.length; i++) {
            if (arr[i] > largest) {
                largest = arr[i];
            }
        }

        System.out.println("Largest element is: " + largest);
    }
}